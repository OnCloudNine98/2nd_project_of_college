<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/UpperArea_main"
        android:layout_width="match_parent"
        android:layout_height="170dp"
        android:background="#297FB8"
        android:orientation="vertical"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent">


    </androidx.constraintlayout.widget.ConstraintLayout>

    <TextView
        android:id="@+id/UpperBlank"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:gravity="center"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <TextView
        android:id="@+id/pageTitle"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="MYCAR"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:layout_margin="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@android:color/transparent"
        android:src="@drawable/main_car_info_arrow_left"
        android:layout_marginLeft="50dp"
        app:layout_constraintTop_toBottomOf="@id/UpperBlank"
        app:layout_constraintBottom_toBottomOf="@id/UpperArea_main"
        app:layout_constraintLeft_toLeftOf="@id/UpperArea_main"
        />

    <ImageButton
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:background="@android:color/transparent"
        android:src="@drawable/main_car_info_arrow_right"
        android:layout_marginRight="50dp"

        app:layout_constraintTop_toBottomOf="@id/UpperBlank"
        app:layout_constraintBottom_toBottomOf="@id/UpperArea_main"
        app:layout_constraintRight_toRightOf="@id/UpperArea_main"
        />


    <TextView
        android:id="@+id/carName_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:text="00가 0000"
        android:textSize="30dp"
        android:layout_margin="10dp"
        app:layout_constraintTop_toBottomOf="@id/UpperBlank"
        app:layout_constraintBottom_toBottomOf="@id/UpperArea_main"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        />

    <TextView
        android:id="@+id/carNum_main"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:textSize="20dp"
        android:layout_margin="10dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        />



    <TextView
        android:id="@+id/maintenance_date_title"
        android:layout_width="170dp"
        android:layout_height="100dp"
        android:text="다음 차량 정비 날짜"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:gravity="top"
        tools:ignore="RtlCompat"
        android:background="@drawable/round2"

        android:layout_margin="20dp"

        app:layout_constraintTop_toBottomOf="@id/UpperArea_main"
        app:layout_constraintLeft_toLeftOf="parent"
         />

        <TextView
            android:id="@+id/maintenance_date_info"
            android:layout_width="170dp"
            android:layout_height="100dp"
            android:text="0월 0일"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:textAlignment="center"
            android:gravity="center"
            tools:ignore="RtlCompat"
            android:layout_margin="20dp"
            app:layout_constraintTop_toBottomOf="@id/UpperArea_main"
            app:layout_constraintLeft_toLeftOf="parent"
            />

    <TextView
        android:id="@+id/engineoil_date_title"
        android:layout_width="170dp"
        android:layout_height="100dp"
        android:text="엔진오일 교체 날짜"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:gravity="top"
        tools:ignore="RtlCompat"
        android:background="@drawable/round2"
        android:layout_margin="20dp"

        app:layout_constraintTop_toBottomOf="@id/UpperArea_main"
        app:layout_constraintRight_toRightOf="parent"
        />

        <TextView
            android:id="@+id/engineoil_date_info"
            android:layout_width="170dp"
            android:layout_height="100dp"
            android:text="0월 0일"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:textAlignment="center"
            android:gravity="center"
            tools:ignore="RtlCompat"

            android:layout_margin="20dp"


            app:layout_constraintTop_toBottomOf="@id/UpperArea_main"
            app:layout_constraintRight_toRightOf="parent"
            />


    <TextView
        android:id="@+id/changingPPF_date_title"
        android:layout_width="170dp"
        android:layout_height="100dp"
        android:text="PPF필름 교체 날짜"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:gravity="top"
        tools:ignore="RtlCompat"
        android:background="@drawable/round2"

        android:layout_margin="20dp"

        app:layout_constraintTop_toBottomOf="@id/maintenance_date_title"
        app:layout_constraintLeft_toLeftOf="parent"
        />

        <TextView
            android:id="@+id/changingPPF_date_info"
            android:layout_width="170dp"
            android:layout_height="100dp"
            android:text="0월 0일"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:textAlignment="center"
            android:gravity="center"
            tools:ignore="RtlCompat"

            android:layout_margin="20dp"


            app:layout_constraintTop_toBottomOf="@id/maintenance_date_title"
            app:layout_constraintLeft_toLeftOf="parent"
            />



    <TextView
        android:id="@+id/changingtire_date_title"
        android:layout_width="170dp"
        android:layout_height="100dp"
        android:text="타이어 교체 날짜"
        android:textSize="20dp"
        android:textColor="@color/black"
        android:textAlignment="center"
        android:gravity="top"
        tools:ignore="RtlCompat"
        android:background="@drawable/round2"

        android:layout_margin="20dp"

        app:layout_constraintTop_toBottomOf="@id/maintenance_date_title"
        app:layout_constraintRight_toRightOf="parent"
        />

        <TextView
            android:id="@+id/changingtire_date_info"
            android:layout_width="170dp"
            android:layout_height="100dp"
            android:text="0월 0일"
            android:textSize="20dp"
            android:textColor="@color/black"
            android:textAlignment="center"
            android:gravity="center"
            tools:ignore="RtlCompat"

            android:layout_margin="20dp"


            app:layout_constraintTop_toBottomOf="@id/maintenance_date_title"
            app:layout_constraintRight_toRightOf="parent"
            />


    <Button
        android:id="@+id/goto_input_info"
        android:layout_width="350dp"
        android:layout_height="50dp"
        android:background="@drawable/round"
        android:backgroundTint="#273238"
        android:text="차량정보 입력"
        android:textAlignment="center"
        android:layout_marginBottom="15sp"
        app:layout_constraintBottom_toTopOf="@+id/constraintLayout"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"></Button>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:background="#E0E0E0"
        app:layout_constraintBottom_toBottomOf="parent">

        <ImageButton
            android:id="@+id/homeButton"
            android:layout_width="23dp"
            android:layout_height="26dp"
            android:layout_marginLeft="50dp"
            android:layout_marginTop="15dp"
            android:background="@android:color/transparent"
            android:src="@drawable/home_test"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageButton
            android:id="@+id/mapButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="70sp"
            android:layout_marginTop="15sp"
            android:background="@android:color/transparent"
            android:src="@drawable/feedback_test"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toEndOf="@+id/homeButton"
            app:layout_constraintTop_toTopOf="parent" />

        >

        <ImageButton
            android:id="@+id/guideButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="70sp"
            android:layout_marginTop="15sp"
            android:background="@android:color/transparent"
            android:src="@drawable/message_test"
            app:layout_constraintLeft_toLeftOf="parent"
            app:layout_constraintStart_toEndOf="@+id/mapButton"
            app:layout_constraintTop_toTopOf="parent" />


        <ImageButton
            android:id="@+id/userButton"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="70sp"
            android:layout_marginTop="15sp"
            android:background="@android:color/transparent"
            android:src="@drawable/people_test"
            app:layout_constraintStart_toEndOf="@+id/guideButton"
            app:layout_constraintTop_toTopOf="parent" />


    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>